:GC=1 n="\n"
:UY=COS :GP *COS :GR
:UX=(COS :GP *COS :GY *-SIN :GR)+(SIN :GP *SIN :GY *COS :GR)
:UZ=(COS :GY *SIN :GP *-COS :GR)+(SIN :GY *-SIN :GR)
:VY=-SIN :GR
:VX=(SIN :GY *SIN :GP *-SIN :GR)+(COS :GY *-COS :GR)
:VZ=(COS :GY *SIN :GP *-SIN :GR)-(SIN :GY *COS :GR)
:EY=SIN :GP
:EX=-SIN :GY *COS :GP
:EZ=COS :GY *COS :GP  goto2
//GC is CyroCalibrate
//Gpyr are pitch roll yaw
//Exyz is Jxyz
//Vxyz is Hxyz
//Uxyz is Ixyz
